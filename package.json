{
  "name": "tsed-serverless",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "hook:before:package:initialize": "yarn run build",
    "hook:before:offline:start": "yarn run build"
  },
  "dependencies": {
    "@tsed/ajv": "^6.92.0",
    "@tsed/common": "^6.92.0",
    "@tsed/core": "^6.92.0",
    "@tsed/di": "^6.92.0",
    "@tsed/exceptions": "^6.92.0",
    "@tsed/json-mapper": "^6.92.0",
    "@tsed/platform-express": "^6.92.0",
    "@tsed/platform-serverless-http": "^6.92.0",
    "@tsed/schema": "^6.92.0",
    "@tsed/swagger": "^6.92.0",
    "ajv": "^8.8.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "serverless-hooks": "^1.0.0",
    "serverless-http": "^2.7.0",
    "serverless-offline": "^8.3.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.15.0",
    "@tsed/cli-plugin-jest": "3.15.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.1",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default",
    "architecture": "default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  }
}
