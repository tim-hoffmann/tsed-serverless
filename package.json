{
  "name": "tsed-serverless",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn tsc && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec babel-node --extensions .ts src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "start:sls": "sls offline start",
    "bundle": "yarn tsc && cross-env NODE_ENV=production webpack",
    "start:bundle": "cross-env NODE_ENV=production node dist/app.bundle.js"
  },
  "dependencies": {
    "@tsed/ajv": "^6.33.1",
    "@tsed/common": "^6.33.1",
    "@tsed/core": "^6.33.1",
    "@tsed/di": "^6.33.1",
    "@tsed/exceptions": "^6.33.1",
    "@tsed/json-mapper": "^6.33.1",
    "@tsed/platform-aws": "^6.33.1",
    "@tsed/platform-express": "^6.33.1",
    "@tsed/schema": "^6.34.2",
    "ajv": "^7.2.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.34",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "serverless": "^2.22.0",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^5.3.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0"
  }
}
